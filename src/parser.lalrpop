use crate::token::*;

grammar;

SemiColon = {
  ";"
}

Arrow = {
  "->"
}

Comma = {
  ","
}

pub Opcode: Opcode = {
  r"mov" => Opcode::Mov,
}

pub Register: Register = {
  r"[rR]0" => Register::R0,
  r"[rR]1" => Register::R1,
}

pub IntLiteral: IntLiteral = {
  r"[0-9]+" => IntLiteral::from(usize::from_str_radix(<>, 10).unwrap())
}



